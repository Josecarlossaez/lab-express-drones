<h2>Available drones</h2>

<div class="lista">
{{#each listDrones}}
<ol>
<h3>Name: {{name}}</h3>
<p>Propellers: {{propellers}}</p>
<p>Max Speed: {{maxSpeed}}</p>
<a href="/drones/{{_id}}/edit"><button>Editar</button></a>
<form action="/drones/{{_id}}/delete" method="POST">
<button class="delete">
Delete
</button>
</form>
</ol>
{{/each}}

</div>